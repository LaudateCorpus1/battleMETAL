textures/test_desert/sand_desert_high_a
{
	qer_editorimage textures/test_desert/sand_desert_high_a.tga

	{
		map textures/test_desert/sand_desert_high_a.tga
		rgbGen identity
	}
	{
    map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
  
}

