textures/ice/snow_a
{
	qer_editorimage textures/ice/snow_a.tga

	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/ice/snow_a.tga
		tcMod scale 1 1
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

